# Install system packages required for Django + PostgreSQL
# Note: psycopg v3 uses binary wheels, no postgresql-devel needed
# Amazon Linux 2023 uses libpq for PostgreSQL client

packages:
  yum:
    gcc: []
    gcc-c++: []
    make: []
    libpq-devel: []
    python3-devel: []
    libffi-devel: []
    openssl-devel: []
    git: []
    jq: []
